<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Who</th>
      <th>Description</th>
      <th>Split</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.encrypted_password %></td>
        <td><%= user.current_sign_in_ip %></td>
      </tr>
    <% end %>
  </tbody>
</table>
