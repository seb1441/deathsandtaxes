<table class="table">
  <thead>
    <tr>
      <th>character_name</th>
      <th>Email</th>
      <th>Last Sign In At</th>
      <th>Split</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.character_name %></td>
        <td><%= user.email %></td>
        <td><%= user.last_sign_in_at %></td>
        <td><%#= user.current_sign_in_ip %></td>
      </tr>
    <% end %>
  </tbody>
</table>
